<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="shivam.happeningapp3" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PathActivity"
            android:label="@string/title_activity_path" >
        </activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail" >
        </activity>
        <activity
            android:name=".TimeTable"
            android:label="@string/title_activity_time_table" >
        </activity>
        <activity
            android:name=".ShowTimeTable"
            android:label="@string/title_activity_show_time_table" >
        </activity>
        <activity
            android:name=".ShowAbout"
            android:label="@string/title_activity_show_about" >
        </activity>
    </application>

</manifest>
